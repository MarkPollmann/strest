{
  "name": "strest",
  "productName": "strest",
  "version": "1.0.0",
  "description": "Electron App to test Http endpoints",
  "main": "./.webpack/main",
  "scripts": {
    "start": "electron-forge start",
    "package": "electron-forge package",
    "make": "electron-forge make",
    "publish": "electron-forge publish",
    "lint": "eslint --ext .ts ."
  },
  "keywords": [],
  "author": {
    "name": "Oscar Franco",
    "email": "ospfranco@protonmail.com"
  },
  "license": "MIT",
  "config": {
    "forge": {
      "packagerConfig": {
        "icon": "./assets/icons/smiley"
      },
      "makers": [
        {
          "name": "@electron-forge/maker-squirrel",
          "config": {
            "name": "strest"
          }
        },
        {
          "name": "@electron-forge/maker-zip",
          "platforms": [
            "darwin",
            "linux"
          ]
        },
        {
          "name": "@electron-forge/maker-dmg",
          "config": {
            "format": "ULFO"
          }
        },
        {
          "name": "@electron-forge/maker-deb",
          "config": {}
        },
        {
          "name": "@electron-forge/maker-rpm",
          "config": {}
        }
      ],
      "plugins": [
        [
          "@electron-forge/plugin-webpack",
          {
            "mainConfig": "./webpack.main.config.js",
            "renderer": {
              "config": "./webpack.renderer.config.js",
              "entryPoints": [
                {
                  "html": "./src/index.html",
                  "js": "./src/renderer.tsx",
                  "name": "main_window"
                }
              ]
            }
          }
        ]
      ]
    }
  },
  "devDependencies": {
    "@electron-forge/cli": "6.0.0-beta.49",
    "@electron-forge/maker-deb": "6.0.0-beta.49",
    "@electron-forge/maker-dmg": "^6.0.0-beta.49",
    "@electron-forge/maker-rpm": "6.0.0-beta.49",
    "@electron-forge/maker-squirrel": "6.0.0-beta.49",
    "@electron-forge/maker-zip": "6.0.0-beta.49",
    "@electron-forge/plugin-webpack": "6.0.0-beta.49",
    "@marshallofsound/webpack-asset-relocator-loader": "^0.5.0",
    "@types/better-queue": "^3.8.2",
    "@types/chance": "^1.0.8",
    "@types/chart.js": "^2.9.12",
    "@types/lodash": "^4.14.149",
    "@types/react": "^16.9.19",
    "@types/react-dom": "^16.9.5",
    "@types/react-redux": "^7.1.7",
    "@types/redux": "^3.6.0",
    "@types/uuid": "^3.4.7",
    "@typescript-eslint/eslint-plugin": "^2.18.0",
    "@typescript-eslint/parser": "^2.18.0",
    "css-loader": "^3.0.0",
    "electron": "8.0.0",
    "eslint": "^6.8.0",
    "eslint-plugin-import": "^2.20.0",
    "eslint-plugin-react": "^7.18.3",
    "fork-ts-checker-webpack-plugin": "^3.1.1",
    "node-loader": "^0.6.0",
    "postcss-loader": "^3.0.0",
    "style-loader": "^0.23.1",
    "tailwindcss": "^1.1.4",
    "ts-loader": "^6.2.1",
    "typescript": "^3.7.0"
  },
  "dependencies": {
    "@types/react-beautiful-dnd": "^12.1.1",
    "better-queue": "^3.8.10",
    "better-queue-memory": "^1.0.4",
    "chance": "^1.1.4",
    "chart.js": "^2.9.3",
    "codemirror": "^5.51.0",
    "electron-squirrel-startup": "^1.0.0",
    "immer": "^5.3.4",
    "lodash": "^4.17.15",
    "react": "^16.12.0",
    "react-beautiful-dnd": "^12.2.0",
    "react-chartjs-2": "^2.9.0",
    "react-codemirror2": "^6.0.0",
    "react-dom": "^16.12.0",
    "react-redux": "^7.1.3",
    "redux": "^4.0.5",
    "redux-persist": "^6.0.0",
    "uuid": "^3.4.0"
  }
}
